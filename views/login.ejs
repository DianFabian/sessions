<%- include("templates/encabezado")%>
<%- include("templates/menu") %>
<h1>Ingresar Cuenta</h1>
<form action="/validar" method="post" id="formulario">
    <div>
        <input type="text" name="usuario" id="usuario" placeholder="Usuario" autofocus>
        <span id="mensajeUsuario"></span>
    </div>

    <div>
        <input type="password" name="password" id="password" placeholder="Password">
        <span id="mensajePassword"></span>
    </div>

    <div>
        <button type="submit">Iniciar sesion</button>
    </div>
</form>
